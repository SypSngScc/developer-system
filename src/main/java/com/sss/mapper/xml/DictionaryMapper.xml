<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sss.mapper.DictionaryDao">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.sss.entity.Dictionary">
		<id column="id" property="id" />
		<result column="dict_name" property="dictName" />
		<result column="dict_value" property="dictValue" />
		<result column="type" property="type" />
		<result column="memo" property="memo" />
	</resultMap>
    
    <select id="getDicValueByDicName" resultType="string" parameterType="string">
        select dicValue from t_dictionary
        where dicName = #{dicName}
    </select>

</mapper>
